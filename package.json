{
  "name": "vite-advance-api-pro",
  "version": "0.0.4",
  "description": "Advanced API plugin for Vite with multiple routing styles, ORM support, and Socket.IO integration",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/",
    "prepublishOnly": "npm run build"
  },
  "type": "module",
  "keywords": [
    "vite",
    "plugin",
    "api",
    "express",
    "router",
    "socket.io",
    "typeorm",
    "prisma",
    "mongodb",
    "mysql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "axios": "^1.3.4",
    "chalk": "^5.4.1",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "glob": "^7.2.0",
    "lodash": "^4.17.21",
    "mongoose": "^7.8.6",
    "mysql2": "^3.12.0",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.7.0",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "@prisma/client": "^5.0.0",
    "vite": "^4.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^22.13.5",
    "@types/socket.io": "^3.0.0",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "esbuild": "^0.25.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "vite": "^5.4.14",
    "vite-plugin-dts": "^3.9.1"
  },
  "engines": {
    "node": ">=14"
  },
  "optionalDependencies": {
    "mongoose": "^7.4.1",
    "mysql2": "^3.5.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  },
  "ts-node": {
    "transpileOnly": true,
    "compilerOptions": {
      "module": "CommonJS"
    }
  }
}
